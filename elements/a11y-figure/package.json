{
  "name": "@lrnwebcomponents/a11y-figure",
  "wcfactory": {
    "className": "A11yFigure",
    "customElementClass": "LitElement",
    "elementName": "a11y-figure",
    "generator-wcfactory-version": "0.7.1",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/a11y-figure.css",
      "scss": "src/a11y-figure.scss",
      "html": "src/a11y-figure.html",
      "js": "src/a11y-figure.js",
      "properties": "src/a11y-figure-properties.json",
      "hax": "src/a11y-figure-hax.json"
    },
    "sharedStyles": [
      "...super.styles"
    ]
  },
  "version": "2.6.13",
  "description": "accessible figure with image description in details",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "a11y-figure.js",
  "module": "a11y-figure.es6.js",
  "umd": "a11y-figure.umd.js",
  "amd": "a11y-figure.amd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/a11y-figure/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "nikkimk"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/a11y-details": "^2.6.12",
    "lit-element": "2.3.1"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "2.0.5",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@webcomponents/webcomponentsjs": "2.4.3",
    "concurrently": "5.2.0",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.5",
    "lodash": "4.17.15",
    "polymer-build": "3.1.4",
    "polymer-cli": "1.9.11",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
