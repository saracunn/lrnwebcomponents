{
  "name": "@lrnwebcomponents/lrndesign-avatar",
  "wcfactory": {
    "className": "LrndesignAvatar",
    "customElementClass": "LitElement",
    "elementName": "lrndesign-avatar",
    "generator-wcfactory-version": "0.7.1",
    "useHAX": true,
    "useSass": false,
    "files": {
      "css": "src/lrndesign-avatar.css",
      "scss": "src/lrndesign-avatar.scss",
      "html": "src/lrndesign-avatar.html",
      "js": "src/lrndesign-avatar.js",
      "properties": "src/lrndesign-avatar-properties.json",
      "hax": "src/lrndesign-avatar-hax.json"
    },
    "sharedStyles": [
      "...super.styles"
    ]
  },
  "version": "2.6.12",
  "description": "Visualize a user account either with an image, a label, or as abstract art.",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "lrndesign-avatar.js",
  "module": "lrndesign-avatar.es6.js",
  "umd": "lrndesign-avatar.umd.js",
  "amd": "lrndesign-avatar.amd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/lrndesign-avatar/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "nikkimk"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/paper-avatar": "^2.6.12",
    "@lrnwebcomponents/simple-colors": "^2.6.12",
    "lit-element": "^2.3.0"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "^2.6.12",
    "@lrnwebcomponents/storybook-utilities": "^2.6.12",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@webcomponents/webcomponentsjs": "2.4.3",
    "concurrently": "5.2.0",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.5",
    "lodash": "4.17.15",
    "polymer-cli": "1.9.11",
    "wct-browser-legacy": "1.0.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
